{
  "family": "masa-oracle-service-task",
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "masa-oracle-service-container",
      "image": "<IMAGE_URI>",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4001,
          "hostPort": 0
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/masa-oracle-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "secrets": [
        {
          "name": "BOOTNODES",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:BOOTNODES"
        },
        {
          "name": "RPC_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:RPC_URL"
        },
        {
          "name": "ENV",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:ENV"
        },
        {
          "name": "FILE_PATH",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:FILE_PATH"
        },
        {
          "name": "WRITER_NODE",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:WRITER_NODE"
        },
        {
          "name": "CACHE_PATH",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:CACHE_PATH"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:PORT"
        },
        {
          "name": "CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:CLIENT_ID"
        },
        {
          "name": "CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:CLIENT_SECRET"
        },
        {
          "name": "CALLBACK_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:CALLBACK_URL"
        },
        {
          "name": "REDIRECT_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:REDIRECT_URL"
        },
        {
          "name": "SESSION_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:SESSION_SECRET"
        },
        {
          "name": "BNB_TOKEN_ADDRESS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:BNB_TOKEN_ADDRESS"
        },
        {
          "name": "MASA_IDENTITY_CONTRACT_ADDRESS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:MASA_IDENTITY_CONTRACT_ADDRESS"
        },
        {
          "name": "DATAFI_EVENT_CONTRACT_ADDRESS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:DATAFI_EVENT_CONTRACT_ADDRESS"
        },
        {
          "name": "BNB_RPC_PROVIDER",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:BNB_RPC_PROVIDER"
        },
        {
          "name": "IP_API_BASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:IP_API_BASE_URL"
        },
        {
          "name": "IP_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:IP_API_KEY"
        },
        {
          "name": "PRIVATE_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:PRIVATE_KEY"
        },
        {
          "name": "REPUTATION_CONTRACT",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:REPUTATION_CONTRACT"
        },
        {
          "name": "SUBNET_PRIVATE_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:SUBNET_PRIVATE_KEY"
        },
        {
          "name": "SUBNET_RPC_PROVIDER",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:SUBNET_RPC_PROVIDER"
        },
        {
          "name": "DB_CONNECTION_NAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:DB_CONNECTION_NAME"
        },
        {
          "name": "SQL_PROXY_CREDENTIALS",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:<AWS_ACCOUNT_ID>:secret:SQL_PROXY_CREDENTIALS"
        }
      ]
    }
  ]
}

